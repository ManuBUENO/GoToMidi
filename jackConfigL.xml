<!DOCTYPE patchbay>
<patchbay version="0.4.2" name="jackConfig">
 <output-sockets>
  <socket exclusive="off" name="Qtractor_metro" client="a2j" type="jack-midi">
   <plug>Qtractor \[130\] \(capture\): Metronome</plug>
  </socket>
  <socket exclusive="off" name="GO_MIDI_OUT" client="RtMidi Output Client" type="jack-midi">
   <plug>MIDI_GO_OUT</plug>
  </socket>
  <socket exclusive="off" name="Through Jack" client="Midi Through" type="jack-midi">
   <plug>Midi Through Port-0</plug>
  </socket>
  <socket exclusive="off" name="AKAI" client="MPK Mini Mk II" type="alsa-midi">
   <plug>MPK Mini Mk II MIDI 1</plug>
  </socket>
  <socket exclusive="off" name="Qtractor Audio Master" client="Qtractor" type="jack-audio">
   <plug>Master/out_1</plug>
   <plug>Master/out_2</plug>
  </socket>
  <socket exclusive="off" name="Audio Jack Sink" client="PulseAudio JACK Sink" type="jack-audio">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket exclusive="off" name="GO_MIDI_METRO" client="RtMidi Input Client" type="jack-midi">
   <plug>MIDI_GO_IN</plug>
  </socket>
  <socket exclusive="off" name="Qtractor_Master" client="a2j" type="jack-midi">
   <plug>Qtractor \[130\] \(playback\): Master</plug>
  </socket>
  <socket exclusive="off" name="Midi Monitor" client="RtMidi Input Client" type="jack-midi">
   <plug>RtMidi Input</plug>
  </socket>
  <socket exclusive="off" name="Through Alsa" client="Midi Through" type="alsa-midi">
   <plug>Midi Through Port-0</plug>
  </socket>
  <socket exclusive="off" name="system Audio Out" client="system" type="jack-audio">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable output="Qtractor_metro" input="GO_MIDI_METRO" type="jack-midi"/>
  <cable output="GO_MIDI_OUT" input="Qtractor_Master" type="jack-midi"/>
  <cable output="Qtractor Audio Master" input="system Audio Out" type="jack-audio"/>
  <cable output="Audio Jack Sink" input="system Audio Out" type="jack-audio"/>
  <cable output="AKAI" input="Through Alsa" type="alsa-midi"/>
  <cable output="Through Jack" input="Midi Monitor" type="jack-midi"/>
  <cable output="Through Jack" input="Qtractor_Master" type="jack-midi"/>
 </cables>
</patchbay>

